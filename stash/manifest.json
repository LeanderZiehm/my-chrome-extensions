{
  "manifest_version": 3,
  "name": "Read Later Saver",
  "version": "1.0",
  "action": {
    "default_icon": "icon128.png"
  },
  	"icons":{"48":"icon48.png", "128":"icon128.png"},
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": [
      "icons/*.png",
      "sounds/*.mp3"
    ],
    "matches": ["<all_urls>"]
  }]
}
